{
  "name": "interview_questions_helper",
  "version": "1.0.0",
  "description": "A bunch of utilities for automating the process of interviews as much as possible with minimal time resources.",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint . --ext .ts,.js --fix && prettier -c '**/*.{ts,js,json,yml,md}' --write",
    "verify": "yarn compile && yarn lint",
    "validateQuestionsDB": "ts-node index.ts --validateQuestionsDB",
    "findQuestions": "ts-node index.ts --findQuestions",
    "generateInterviewPDF": "ts-node index.ts --generateInterviewPDF",
    "generateResultPDF": "ts-node index.ts --generateResultPDF"
  },
  "keywords": [
    "riseapps",
    "interviews",
    "react-native"
  ],
  "author": "Dmitry Usik",
  "license": "ISC",
  "dependencies": {
    "chart.js": "^2.9.4",
    "lodash.startcase": "^4.4.0",
    "node-fetch": "^2.6.1",
    "pdfkit": "^0.11.0",
    "quickchart-js": "^1.0.5"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.29",
    "@types/lodash.startcase": "^4.4.6",
    "@types/node": "^14.14.21",
    "@types/node-fetch": "^2.5.8",
    "@types/pdfkit": "^0.10.6",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
