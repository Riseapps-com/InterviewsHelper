{
  "name": "InterviewsHelper",
  "version": "1.0.0",
  "description": "A bunch of utilities for automating the process of interviews as much as possible with minimal time resources.",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint . --ext .ts,.js --fix && prettier -c '**/*.{ts,js,json,yml,md}' --write",
    "verify": "yarn compile && yarn lint",
    "validateQuestionsDB": "ts-node index.ts --validateQuestionsDB",
    "generateQuestions": "ts-node index.ts --generateQuestions",
    "generateInterviewPDF": "ts-node index.ts --generateInterviewPDF",
    "generateResultPDF": "ts-node index.ts --generateResultPDF"
  },
  "keywords": [
    "riseapps",
    "interviews",
    "react-native"
  ],
  "author": "Dmitry Usik",
  "license": "ISC",
  "devDependencies": {
    "@types/chart.js": "^2.9.32",
    "@types/lodash.startcase": "^4.4.6",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/pdfkit": "^0.11.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chart.js": "^3.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jest-formatting": "^2.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash.startcase": "^4.4.0",
    "moment-timezone": "^0.5.33",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^3.1.5",
    "pdfkit": "^0.12.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "quickchart-js": "^1.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
