{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "isolatedModules": false,
        "baseUrl": "./",
        "sourceMap": true
    }
}
